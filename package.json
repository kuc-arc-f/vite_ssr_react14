{
  "name": "@vitejs/test-ssr-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "NODE_ENV=develop node server",
    "build": "npm run build:client && npm run build:server",
    "build:test": "vite build --outDir public/static",
    "build:client": "vite build --mode client --outDir public/client",
    "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
    "build:ssr": "vite build --ssr index.js --outDir dist/ssr",
    "generate": "vite build --outDir dist/static && npm run build:server && node prerender",
    "clean": "rimraf dist && rimraf public/static",
    "serve": "NODE_ENV=production node server",
    "start": "NODE_ENV=production node index.js",
    "debug": "node --inspect-brk server"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "rimraf": "^5.0.5",
    "serve-static": "^1.15.0"
  }
}
